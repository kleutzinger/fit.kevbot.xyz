{
  "name": "tracker.kevbot.xyz",
  "version": "1.0.0",
  "description": "track my workouts",
  "type": "module",
  "main": "src/index.js",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "node test/test.js",
    "build": "tailwindcss -i ./src/style.css -o ./src/output.css",
    "dev": "concurrently \"npm:watch-node\" \"npm:watch-tailwind\"",
    "watch-node": "nodemon src/index.js",
    "watch-tailwind": "tailwindcss -i ./src/style.css -o ./src/output.css --watch"
  },
  "author": "Kevin Leutzinger",
  "license": "MIT",
  "dependencies": {
    "@json2csv/plainjs": "^7.0.3",
    "better-sqlite3": "^8.6.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "express-openid-connect": "^2.17.0",
    "htmx.org": "^1.9.6",
    "javascript-time-ago": "^2.5.9",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.6",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "tailwindcss": "^3.3.3"
  }
}
