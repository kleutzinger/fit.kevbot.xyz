{
  "name": "tracker.kevbot.xyz",
  "version": "1.0.0",
  "description": "track my workouts",
  "type": "module",
  "main": "src/index.js",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start": "node src/index.js",
    "prestart": "npm run init-db",
    "test": "node test/test.js",
    "build": "tailwindcss -i ./src/input.css -o ./public/style.css",
    "dev": "concurrently \"npm:watch-node\" \"npm:watch-tailwind\"",
    "init-db": "node src/init-db.js",
    "watch-node": "nodemon src/index.js",
    "watch-tailwind": "tailwindcss -i ./src/input.css -o ./public/style.css --watch"
  },
  "author": "Kevin Leutzinger",
  "license": "MIT",
  "dependencies": {
    "@blackglory/better-sqlite3-migrations": "^0.1.17",
    "@json2csv/plainjs": "^7.0.3",
    "better-sqlite3": "^8.6.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "express-openid-connect": "^2.17.0",
    "htmx.org": "^1.9.6",
    "javascript-time-ago": "^2.5.9",
    "lodash": "^4.17.21",
    "migration-files": "^0.4.1",
    "run-func": "^3.0.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.6",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "tailwindcss": "^3.3.3"
  }
}
